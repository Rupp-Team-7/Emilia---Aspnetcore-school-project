{
    "version": "2.0.0",
    "tasks": [
        {
            "taskName": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Emilia/Emilia.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "taskName": "build fast",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Emilia/Emilia.csproj",
                "--no-restore"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}